{
    "extends": [
      "tslint:recommended",
      "tslint-react",
      "tslint-config-prettier"
    ],
    "rules": {
      "arrow-parens": [true, "ban-single-arg-parens"],
      "interface-name": false,
      "object-literal-sort-keys": false,
      "jsx-boolean-value": false,
      "ordered-imports": false,
      "jsx-no-lambda": false
    }
  }